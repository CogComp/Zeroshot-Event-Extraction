{
    "use_gpu": true,
    "gpu_devices": "0",
    "bert_cache_dir": "/shared/.cache/transformers",
    "root_dir": "/shared/lyuqing/probing_for_event",
    "input_file": "data/ACE_oneie/en/event_only/dev.event.json",
    "srl_model": "['celine_old', 'celine_new', 'illinois', 'celine_new_all'][3]",

    "classification_only": false,
    "gold_trigger": true,

    "YN_QA_model_name": "boolq_roberta",

    "EX_QA_model_name": "qamr_roberta-l",
    "allow_na": true,
    "null_score_diff_threshold": 0.0,
    "top_k_args": 10,

    "srl_args": "[['V'], ['V','ARG0','ARG1'], ['V','ARG0','ARG1','ARG2'], ['V','ARG0'], ['V', 'ARG1'], 'all'][-1]",
    "trg_thresh": "[0.80, 0.85, 0.900, 0.95, 0.97, 0.98, 0.99, 0.995][-2]",
    "arg_thresh": "[0.0, 0.1, 0.3, 0.5, 0.6, 0.65, 0.7, 0.75, 0.8, 0.85, 0.900, 0.95, 0.99][0]",
    "predicate_type": "[['verb'], ['nom'], ['verb','nom']][-1]",
    "add_neutral": true,
    "identify_head": true,
    "tune_on_gdl": "[True, False][1]",
    "const_premise": "['false', 'whenNone', 'alwaystrg'][1]",
    "pair_premise_strategy": "[None, 'max_delta', 'max_conf+delta'][0]",

    "arg_probe_type":"['bool', 'ex', 'ex_wtrg','ex_wtrg+','ex_wtrg_type'][4]"

}
