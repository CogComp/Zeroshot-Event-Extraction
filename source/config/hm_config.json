{
    "use_gpu": true,
    "gpu_devices": "5",
    "bert_cache_dir": "/shared/.cache/transformers",
    "root_dir": "/shared/lyuqing/probing_for_event",
    "input_file": "data/ACE_oneie/en/event_only/dev.event.json",

    "classification_only": true,

    "bert_model_type": "robertal",
    "srl_args": "[['V'], ['V','ARG0','ARG1'], ['V','ARG0','ARG1','ARG2'], ['V','ARG0'], ['V', 'ARG1'], 'all'][-2]",
    "trg_thresh": "[0.80, 0.85, 0.900, 0.95, 0.97, 0.98, 0.99][-1]",
    "arg_thresh": "[0.900, 0.95, 0.99][0]",
    "predicate_type": "[['verb'], ['nom'], ['verb','nom']][-1]",
    "add_neutral": true,

    "trg_probe_type": "['topical', 'natural', 'exist', 'prem-trg+type'][0]",
    "pair_premise_strategy": "[None, 'max_delta', 'max_conf+delta'][2]",
    "const_premise": "['false', 'whenNone', 'alwaystrg'][1]",
    "tune_on_gdl": "[None, 'pos_only', 'pos_neg'][0]",

    "arg_probe_type": "['manual','auto_issth', 'auto_sthis'][0]",
    "identify_head": true

}
